{
  "name": "gulp-i18n-update-localization-ids",
  "version": "1.1.0",
  "description": "Update i18n localization ids in html files.",
  "keywords": [
    "gulp",
    "i18n",
    "update",
    "localization",
    "id",
    "html",
    "aurelia"
  ],
  "main": "index.js",
  "scripts": {
    "test": "nyc -- ava --verbose",
    "watch": "ava --watch --verbose",
    "prepack": "npm test",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "devDependencies": {
    "ava": "^1.1.0",
    "coveralls": "^3.0.2",
    "gulp": "^4.0.0",
    "nyc": "^13.1.0"
  },
  "peerDependencies": {
    "gulp": "^4.0.0",
    "vinyl": "^2.0.0"
  },
  "dependencies": {
    "parse5": "^5.1.0",
    "plugin-error": "^1.0.1",
    "through2": "^3.0.0"
  },
  "ava": {
    "files": [
      "test/*.js"
    ]
  },
  "nyc": {
    "reporter": [
      "text",
      "text-summary",
      "lcov"
    ]
  },
  "repository": "github:Netatwork-de/gulp-i18n-update-localization-ids",
  "author": "Net at Work GmbH",
  "license": "Apache-2.0"
}
